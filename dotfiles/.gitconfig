[core]
	excludesfile = /Users/gpadmin/.gitignore_global
	hooksPath = ${HOME}/workspace/git-hooks-core
	editor = vim
	pager = less -r
[difftool "sourcetree"]
	cmd = /usr/local/bin/bcomp \"$LOCAL\" \"$REMOTE\"
	path = -ro
[mergetool "sourcetree"]
	cmd = /usr/local/bin/bcomp \"$LOCAL\" \"$REMOTE\" \"$BASE\" \"$MERGED\"
	trustExitCode = true
[user]
	name = Ming Li
	email = mli@apache.org
[commit]
[icdiff]
	options = --highlight --line-numbers
[diff]
	tool = bc3
[merge]
	tool = bc3
[mergetool "bc3"]
	trustExitCode = true
[alias]
