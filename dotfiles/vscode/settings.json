{
  "cmake.configureOnOpen": true,
  "http.proxySupport": "off",
  "go.formatTool": "goimports",
  "sync.gist": "99caa4af83ff584b8897d9b500016253",
  "go.useLanguageServer": true,
  "git.ignoreLegacyWarning": true,
  "extensions.ignoreRecommendations": false,
  "editor.renderControlCharacters": true,
  "workbench.sideBar.location": "left",
  "gitlens.views.repositories.files.layout": "list",
  "scm.alwaysShowRepositories": true,
  "editor.formatOnSaveMode": "modifications",
  "files.autoSave": "onWindowChange",
  "todo-tree.tree.showScanModeButton": false,
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]"
  ],
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "editor.tabSize": 8,
  "editor.cursorStyle": "line",
  "editor.insertSpaces": false,
  "editor.lineNumbers": "on",
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.wordWrap": "off",
  "security.workspace.trust.untrustedFiles": "open",
  "go.toolsManagement.autoUpdate": true,
  // 忽略排除的文件
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/__pycache__": true, // python 生产的cache文件
    "**/.idea": true
  },
  "editor.guides.bracketPairs": "active",
  "editor.bracketPairColorization.enabled": true,
  "tabnine.experimentalAutoImports": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "vscode-office.openOutline": true,
  "gitlens.views.commitDetails.files.layout": "list",
  "terminal.integrated.scrollback": 10000,
  "git.openRepositoryInParentFolders": "never",
  "workbench.colorTheme": "Atom One Light",
  "gitlens.hovers.annotations.over": "annotation",
  "editor.inlayHints.enabled": "onUnlessPressed",
  "github-enterprise.uri": "https://cocopilot.org",
  "editor.multiDocumentOccurrencesHighlight": true,
  "editor.fontSize": 14,
  "workbench.activityBar.location": "top",
  "gitlens.graph.minimap.enabled": false,
  "gitlens.ai.experimental.provider": "openai",
  "gitlens.ai.experimental.openai.model": "gpt-4-1106-preview",
  "gitlens.graph.layout": "editor",
  // ----------------- vim config -----------------
  "vim.leader": "<space>",
  "vim.useSystemClipboard": true,
  "vim.hlsearch": true,
  "vim.highlightedyank.enable": true,
  "vim.foldfix": true,
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useCtrlKeys": true,
  "vim.surround": true,
  "vim.sneak": true,
  "vim.sneakUseIgnorecaseAndSmartcase": true,
  "vim.insertModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<C-e>"
      ],
      "after": [],
      "commands": [
        {
          "command": "cursorEnd",
          "args": []
        }
      ]
    },
    {
      "before": [
        "<C-a>"
      ],
      "after": [],
      "commands": [
        {
          "command": "cursorHome",
          "args": []
        }
      ]
    },
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    // ----------------------------------------------------
    // Jump to change
    {
      "before": [
        "[",
        "c"
      ],
      "commands": [
        "workbench.action.editor.previousChange"
      ]
    },
    {
      "before": [
        "]",
        "c"
      ],
      "commands": [
        "workbench.action.editor.nextChange"
      ]
    },
    // Need to install extension: Go to Next/Previous Member
    {
      "before": [
        "[",
        "m"
      ],
      "commands": [
        "gotoNextPreviousMember.previousMember"
      ]
    },
    {
      "before": [
        "]",
        "m"
      ],
      "commands": [
        "gotoNextPreviousMember.nextMember"
      ]
    },
    // Code actions
    {
      "before": [
        "<leader>",
        "s",
        "a"
      ],
      "commands": [
        "editor.action.sourceAction"
      ]
    },
    {
      "before": [
        "<leader>",
        "r",
        "r"
      ],
      "commands": [
        // "extension.runScript"
        // "autojspro.run"
        "code-runner.run"
      ]
    },
    // Quick fix
    {
      "before": [
        "<leader>",
        "q",
        "f"
      ],
      "commands": [
        "editor.action.quickFix"
      ]
    },
    // 重用名变量
    {
      "before": [
        "<leader>",
        "r",
        "n"
      ],
      "commands": [
        "editor.action.rename"
      ]
    },
    // Format 格式化当前文件
    {
      "before": [
        "<leader>",
        "f",
        "m"
      ],
      "commands": [
        "editor.action.formatDocument"
      ]
    },
    // go===================================================
    {
      "before": [
        "g",
        "d"
      ],
      "commands": [
        "editor.action.revealDefinition"
      ]
    },
    {
      "before": [
        "g",
        "D"
      ],
      "commands": [
        "editor.action.goToDeclaration"
      ]
    },
    {
      "before": [
        "g",
        "p",
        "d"
      ],
      "commands": [
        "editor.action.peekDefinition"
      ]
    },
    // [g]o to t[y]pe
    {
      "before": [
        "g",
        "y"
      ],
      "commands": [
        "editor.action.goToTypeDefinition"
      ]
    },
    {
      "before": [
        "g",
        "p",
        "y"
      ],
      "commands": [
        "editor.action.peekTypeDefinition"
      ]
    },
    // go to References
    {
      "before": [
        "g",
        "r"
      ],
      "commands": [
        "editor.action.goToReferences"
      ]
    },
    {
      "before": [
        "g",
        "p",
        "r"
      ],
      "commands": [
        "editor.action.referenceSearch.trigger"
      ]
    },
    {
      "before": [
        "<leader>",
        "g",
        "r"
      ],
      "commands": [
        "references-view.findReferences"
      ],
      "when": "editorHasReferenceProvider"
    },
    // go to impl
    {
      "before": [
        "g",
        "i"
      ],
      "commands": [
        "editor.action.goToImplementation"
      ]
    },
    {
      "before": [
        "g",
        "p",
        "i"
      ],
      "commands": [
        "editor.action.peekImplementation"
      ]
    },
    {
      "before": [
        "<leader>",
        "g",
        "i"
      ],
      "commands": [
        "references-view.findImplementations"
      ],
      "when": "editorHasImplementationProvider"
    },
    // go to call hierarchy
    {
      "before": [
        "g",
        "c"
      ],
      "commands": [
        "editor.showCallHierarchy"
      ],
      "when": "editorHasCallHierarchyProvider && editorTextFocus && !inReferenceSearchEditor"
    },
    {
      "before": [
        "<leader>",
        "h",
        "c"
      ],
      "commands": [
        "references-view.showCallHierarchy"
      ],
      "when": "editorHasCallHierarchyProvider && editorTextFocus && !inReferenceSearchEditor"
    },
    // ===================================================
    // new===================================================
    // 新建文件夹,在编辑器的区域
    {
      "before": [
        "<Leader>",
        "n",
        "d"
      ],
      "commands": [
        "explorer.newFolder"
      ]
    },
    // 新建文件,新建文件的位置取决于,文件资源管理器所在的位置
    {
      "before": [
        "<Leader>",
        "n",
        "f"
      ],
      "commands": [
        "explorer.newFile"
      ]
    },
    // =====================================================
    // open========================================================
    // 打开文件资源管理器,光标会聚焦到文件资源管理器的窗口
    {
      "before": [
        "<leader>",
        "e"
      ],
      "commands": [
        "workbench.view.explorer"
      ]
    },
    // 新建一个terminal终端
    {
      "before": [
        "<leader>",
        "n",
        "t"
      ],
      "commands": [
        "workbench.action.terminal.new"
      ]
    },
    // ===============================================
    // 移动
    {
      "before": [
        "<C-h>"
      ],
      "after": [
        "<C-w>",
        "h"
      ]
    },
    {
      "before": [
        "<C-j>"
      ],
      "after": [
        "<C-w>",
        "j"
      ]
    },
    {
      "before": [
        "<C-k>"
      ],
      "after": [
        "<C-w>",
        "k"
      ]
    },
    {
      "before": [
        "<C-l>"
      ],
      "after": [
        "<C-w>",
        "l"
      ]
    },
    {
      "before": [
        "<leader>",
        "h"
      ],
      "commands": [
        "workbench.action.navigateLeft"
      ]
    },
    {
      "before": [
        "<leader>",
        "j"
      ],
      "commands": [
        "workbench.action.navigateDown"
      ]
    },
    {
      "before": [
        "<leader>",
        "k"
      ],
      "commands": [
        "workbench.action.navigateUp"
      ]
    },
    {
      "before": [
        "<leader>",
        "l"
      ],
      "commands": [
        "workbench.action.navigateRight"
      ]
    },
    // ---------------------------------------------------------------
    {
      "before": [
        "<leader>",
        "'"
      ],
      "commands": [
        "workbench.action.maximizeEditor"
      ]
    },
    // 定义速览, 默认是：gh, 但是没有focus Hover
    {
      "before": [
        "K"
      ],
      "commands": [
        "editor.action.showDefinitionPreviewHover"
      ]
    },
    //默认是: gq
    {
      "before": [
        "<leader>",
        "q",
        "f"
      ],
      "commands": [
        "editor.action.quickFix"
      ]
    }
  ],
  "editor.renderWhitespace": "all",
  "vim.showMarksInGutter": true,
  "redhat.telemetry.enabled": false,
  "gitlens.views.commits.files.layout": "list",
  "gitProjectManager.baseProjectsFolders": [
    "~/workspace"
  ],
}
